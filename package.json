{
  "name": "micro-status-vigo",
  "description": "Shows status vigo bot",
  "version": "1.1.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.9.3"
  },
  "scripts": {
    "test": "standard && nsp check && tap --reporter=spec test/**/*.js",
    "test-offline": "standard && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "start": "micro -p 8080",
    "dev": "micro-dev",
    "now-deploy": "npm test && now --npm && now alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "status",
    "vigo",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/micro-status-vigo.git"
  },
  "bugs": {
    "url": "https://github.com/telemarks/micro-status-vigo/issues"
  },
  "homepage": "https://github.com/telemarks/micro-status-vigo#readme",
  "now": {
    "alias": "vigo.status.t-fk.win",
    "env": {
      "NODE_ENV": "production"
    },
    "engines": {
      "node": "8.9.3"
    }
  },
  "devDependencies": {
    "coveralls": "3.0.0",
    "micro-dev": "2.2.0",
    "nsp": "2.6.3",
    "standard": "11.0.0",
    "tap": "11.1.0"
  },
  "dependencies": {
    "got": "8.1.0",
    "markdown-it": "8.4.1",
    "micro": "9.1.0"
  }
}
